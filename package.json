{
  "name": "great-styles",
  "version": "0.2.0",
  "engines": {
    "node": "12"
  },
  "description": "Styles for great.gov.uk",
  "main": "src/index.js",
  "private": true,
  "scripts": {
    "static-watch": "webpack --watch --mode development",
    "static-build": "webpack --mode production",
    "storybook-manifest": "cp manifest_storybook.yml storybook-static/manifest.yml",
    "storybook-push": "cd storybook-static && cf push great-styles-staging",
    "storybook-staging": "npm run build-storybook && npm run storybook-manifest && npm run storybook-push",
    "storybook": "start-storybook -p 8080 -s static",
    "build-storybook": "build-storybook -s public",
    "dsm-storybook:publish": "dsm-storybook publish",
    "dsm-storybook:preview": "dsm-storybook preview",
    "babel": "./node_modules/.bin/babel src --ignore '**/*.stories.js' --out-dir ./dist"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/preset-env": "^7.14.9",
    "@babel/preset-react": "^7.14.5",
    "@fortawesome/fontawesome-free": "^5.15.3",
    "@storybook/addon-a11y": "6.2.9",
    "@storybook/addon-backgrounds": "6.2.9",
    "@storybook/addon-knobs": "6.2.9",
    "@storybook/addons": "6.2.9",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "6.2.9",
    "babel-loader": "^8.2.2",
    "copy-webpack-plugin": "^9.0.1",
    "css-loader": "^3.6.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "html-webpack-plugin": "5.0.0-alpha.15",
    "mini-css-extract-plugin": "^2.2.0",
    "node-sass": "^6.0.1",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-html-parser": "^2.0.2",
    "remove-files-webpack-plugin": "^1.4.5",
    "sass-loader": "^10.2.0",
    "style-loader": "^1.3.0",
    "webpack-cli": "^4.8.0"
  }
}
